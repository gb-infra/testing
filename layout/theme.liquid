<!--   UTM code  -->
 <script>
            function get(name){
                var r = /[?&]([^=#]+)=([^&#]*)/g,p={},match;
                while(match = r.exec(window.location)) p[match[1]] = match[2];
                return p[name];
            }   
            let parameters = ['utm_source', 'utm_medium', 'utm_content', 'utm_campaign'];
            let now = new Date();
            let minutes = 7 * 24 * 60;
            now.setTime(now.getTime() + (minutes * 60 * 1000));
            for (let parameter of parameters) {
                let parameterValue = get(parameter);
                if(parameterValue) {
                    document.cookie = `${parameter}=${parameterValue}; expires=${now.toUTCString()} UTC; path=/`
                }
            }
  </script>
</script>
